{
  "name": "@slimio/nixmem",
  "version": "1.0.0",
  "description": "UNIX Memory - Node.JS low level binding",
  "main": "index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "prebuilds": "prebuildify --napi",
    "build": "cross-env node-gyp configure && node-gyp build",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Nixmem.git"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env eslint index.js && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "SlimIO",
    "Monitoring",
    "UNIX",
    "Memory",
    "Binding",
    "NAPI"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "include",
    "binding.gyp",
    "nixmem.cpp"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/SlimIO/Nixmem/issues"
  },
  "homepage": "https://github.com/SlimIO/Nixmem#readme",
  "dependencies": {
    "node-addon-api": "^3.0.0",
    "node-gyp-build": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/eslint-config": "^4.1.0",
    "@slimio/is": "^1.5.0",
    "ava": "^3.8.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.32.0",
    "husky": "^4.2.5",
    "node-gyp": "^6.0.1",
    "nyc": "^15.0.1",
    "pkg-ok": "^2.3.1",
    "prebuildify": "^4.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 98,
    "statements": 98,
    "functions": 98,
    "branches": 90
  }
}
